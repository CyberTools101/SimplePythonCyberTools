import os
import sys
from threading import Thread
from pathlib import Path
from base64 import b64encode


print(Path.cwd())
print(os.urandom(100))
print(os.path.basename(__file__))

f = open("newpython.py", "a")
f.write('''
import os
import sys
from threading import Thread
from pathlib import Path
from base64 import b64encode


print(Path.cwd())
print(os.urandom(100))
print(os.path.basename(__file__))

f = open("newpython.py", "a")
f.write(\'\'\'
import os
import sys
from threading import Thread
from pathlib import Path
from base64 import b64encode

'{randomness}'
\'\'\'.format(randomness = b64encode(os.urandom(100)).decode('utf-8')))
f.close()


def runFunc():
    os.system('python3 newpython.py')

thread = Thread(target=runFunc)
thread.start()
print("yep I am here")
sys.exit()
'{randomness}'
'''.format(randomness = b64encode(os.urandom(100)).decode('utf-8')))
f.close()


def runFunc():
    os.system('python3 newpython.py')

thread = Thread(target=runFunc)
thread.start()
print("yep I am here")
sys.exit()
